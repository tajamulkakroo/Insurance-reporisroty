import { test, expect } from '@playwright/test';

test('test', async ({ page }) => {
  await page.goto('https://individualinsurance.healthpartners.com/hp/shopping/anonymous.html#welcome');
  await page.locator('html').click();
  await page.getByRole('link', { name: 'Get started' }).click();
  await page.getByRole('link', { name: 'See if you qualify' }).click();
  await page.getByLabel('Birth of child, adoption of').check();
  await page.getByLabel('Enter the date of your event:').click();
  await page.getByRole('cell', { name: '14' }).click();
  await page.getByRole('checkbox').check();
  await page.getByRole('link', { name: 'Save & Continue' }).click();
  await page.getByLabel('*Zip Code').click();
  await page.getByText('- Minneapolis, MN (Hennepin)').click();
  await page.getByLabel('*First Name').click();
  await page.getByLabel('*First Name').press('Tab');
  await page.getByPlaceholder('MM/DD/YYYY').click();
  await page.getByLabel('*Gender').selectOption('M');
  await page.getByLabel('*Tobacco use').selectOption('n');
  await page.getByRole('link', { name: 'Add Dependent' }).click();
  await page.locator('#firstName38').click();
  await page.locator('#lastName39').click();
  await page.locator('#birthDate40').click();
  await page.locator('#memberRelationship41').selectOption('CHILD');
  await page.locator('#gender42').selectOption('M');
  await page.getByRole('link', { name: 'Continue' }).click();
  await page.getByRole('link', { name: 'Browse all plans' }).click();
  await page.goto('https://individualinsurance.healthpartners.com/hp/shopping/anonymous.html#view/productList/list');
  await page.getByRole('link', { name: 'Add to cart' }).first().click();
  await page.getByRole('link', { name: 'View Cart & Enroll' }).click();
  await page.getByRole('link', { name: 'Enroll Now' }).click();
  await page.goto('https://individualinsurance.healthpartners.com/hp/application/PriorToApplying.action#1');
  await page.goto('https://individualinsurance.healthpartners.com/hp/application/PriorToApplying.action#2');
  await page.goto('https://individualinsurance.healthpartners.com/hp/application/PriorToApplying.action#3');
  await page.goto('https://individualinsurance.healthpartners.com/hp/application/PriorToApplying.action#4');
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_userProfile_user_emailAddress').click();
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_userProfile_user_emailAddress').fill('test@zyz.com');
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_userId').click();
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_userId').fill('097768666');
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_userId').click();
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_userId').fill('09776866687');
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_password').click();
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_password').fill('Ramazan22');
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_confirmPassword').click();
  await page.locator('#priorToApplyingForm_priorToApplyingInfo_registrationInfo_confirmPassword').fill('Ramazan22');
  await page.getByLabel('*\n                Security question').selectOption('What was your childhood nickname?');
  await page.getByLabel('*\n                Answer').click();
  await page.getByLabel('*\n                Answer').click();
  await page.getByLabel('*\n                Answer').fill('hello');
  await page.getByLabel('No').check();
  await page.goto('https://individualinsurance.healthpartners.com/hp/applicationservice/Application.action?appResponseCode=b3b3e6b1-6bc5-4037-a1b5-01069bce9a47');
  await page.goto('https://individualinsurance.healthpartners.com/hp/applicationservice/Application.action?appResponseCode=b3b3e6b1-6bc5-4037-a1b5-01069bce9a47#NOBACK1');
  await page.goto('https://individualinsurance.healthpartners.com/hp/applicationservice/Application.action?appResponseCode=b3b3e6b1-6bc5-4037-a1b5-01069bce9a47#NOBACK2');
  await page.goto('https://individualinsurance.healthpartners.com/hp/applicationservice/Application.action?appResponseCode=b3b3e6b1-6bc5-4037-a1b5-01069bce9a47#NOBACK1');
  await page.goto('https://individualinsurance.healthpartners.com/hp/applicationservice/Application.action?appResponseCode=b3b3e6b1-6bc5-4037-a1b5-01069bce9a47#NOBACK2');
  await page.goto('https://individualinsurance.healthpartners.com/hp/applicationservice/Application.action?appResponseCode=b3b3e6b1-6bc5-4037-a1b5-01069bce9a47#PAGEC2A9DD39F777FCCCF807');
  await page.getByRole('link', { name: 'Start enrollment ' }).click();
  await page.locator('input[name="Application\\.Applicant\\.Address_1"]').click();
  await page.locator('input[name="Application\\.Applicant\\.Address_1"]').fill('House number 1566');
  await page.locator('input[name="Application\\.Applicant\\.City"]').click();
  await page.locator('input[name="Application\\.Applicant\\.City"]').fill('Chicago');
  await page.locator('input[name="Application\\.Applicant\\.Home_Phone"]').click();
  await page.getByLabel('Check this box if applicant').check();
  await page.getByRole('link', { name: 'Continue ' }).click();
  await page.goto('https://individualinsurance.healthpartners.com/hp/applicationservice/Application.action?appResponseCode=b3b3e6b1-6bc5-4037-a1b5-01069bce9a47#PAGE36C2D0AC7G95A4E40239');
  await page.locator('div').filter({ hasText: /^\*Same address as primary applicant\? YesNo$/ }).getByLabel('Yes').check();
  await page.getByLabel('Check this box if applicant').check();
  await page.getByRole('link', { name: 'Continue ' }).click();
  await page.getByLabel('No').first().check();
  await page.getByLabel('No').nth(2).check();
  await page.getByRole('link', { name: 'Continue ' }).click();
  await page.getByRole('textbox').click();
  await page.getByRole('textbox').setInputFiles('dummy.pdf');
  await page.getByRole('link', { name: 'Continue ' }).click();
  await page.locator('div').filter({ hasText: /^NO$/ }).first().click();
  await page.getByLabel('No, I prefer to get and pay').check();
  await page.getByLabel('Credit/Debit Card').check();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.Cardholder_First_Name"]').click();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.Cardholder_First_Name"]').fill('mr');
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.Cardholder_Last_Name"]').click();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.Cardholder_Last_Name"]').fill('yahoo');
  await page.getByLabel('Visa').check();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.CC_Number"]').click();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.CC_Number"]').fill('1234567890122');
  await page.getByPlaceholder('mm/yyyy').click();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.CC_Number"]').click();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.CC_Number"]').click();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.CC_Number"]').dblclick();
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.CC_Number"]').press('Control+a');
  await page.locator('input[name="Application\\.Initial_Credit_Card\\.CC_Number"]').fill('4111111111111111');
  await page.locator('div').filter({ hasText: /^\*Expiration date:$/ }).click();
  await page.getByPlaceholder('mm/yyyy').click();
  await page.getByLabel('Yes', { exact: true }).check();
  await page.getByRole('link', { name: 'Continue ' }).click();
  await page.getByRole('button', { name: 'Correct errors' }).click();
  await page.getByRole('radio', { name: 'NO', exact: true }).check();
  await page.getByRole('link', { name: 'Continue ' }).click();
  await page.getByRole('link', { name: 'Continue ' }).click();
  await page.getByRole('checkbox', { name: 'I agree with the terms and' }).check();
  await page.getByRole('checkbox', { name: 'I understand that I, or those' }).check();
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.First_Name"]').click();
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.First_Name"]').fill('hello');
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.First_Name"]').press('Tab');
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.Last_Name"]').fill('Hello');
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.Confirm_First_Name"]').click();
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.Confirm_First_Name"]').fill('Hello');
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.Confirm_First_Name"]').press('Tab');
  await page.getByRole('group', { name: 'Electronic Signature -- Mr' }).locator('input[name="Application\\.Signature\\.Confirm_Last_Name"]').fill('Hello');
  await page.getByRole('link', { name: 'Submit enrollment' }).click();
  await page.getByRole('button', { name: 'Correct errors' }).click();
});



 